<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bristol.snorediaby">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".web.view.activities.calculate.CalculateActivity" />
        <activity android:name=".web.view.activities.personal.PersonalActivity" />
        <activity android:name=".web.view.activities.email.EmailActivity" />
        <activity android:name=".web.view.activities.language.LanguageActivity" />
        <activity android:name=".web.view.activities.data.DataActivity" />

        <service android:name=".logic.services.AlarmService" />
    </application>

</manifest>